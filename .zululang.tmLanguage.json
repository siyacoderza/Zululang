{
  "scopeName": "source.zul",
  "patterns": [
    {
      "name": "keyword.control.zul",
      "match": "\\b(uma|kungenjalo|ngenkathi|umsebenzi|buyisa|kuba)\\b"
    },
    {
      "name": "keyword.other.zul",
      "match": "\\b(bonisa|cela)\\b"
    },
    {
      "name": "string.quoted.double.zul",
      "match": "\".*?\""
    },
    {
      "name": "constant.numeric.zul",
      "match": "\\b\\d+\\b"
    }
  ]
}
